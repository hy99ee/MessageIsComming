# platform :ios, '13.0'

def rx_pods
    pod 'RxSwift', '6.5.0'
    pod 'RxCocoa', '6.5.0'
    pod 'RxGesture'
end

def ui_pods
    pod 'SnapKit', '5.6.0'
    pod 'Hero', '1.6.1'
    pod 'VisualEffectView', '4.1.3'
    pod 'SwiftyAttributes', '5.1.1'
end

target 'MessageIsComming' do
  use_frameworks! # Comment the next line if you don't want to use dynamic frameworks
    rx_pods
    ui_pods
end
